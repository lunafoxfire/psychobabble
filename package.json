{
  "name": "soft-skill-testing",
  "version": "0.0.0",
  "scripts": {
    "start": "concurrently --names \"node,ng\" \"npm run serve:dev\" \"npm run ng-watch\"",
    "start:prod": "concurrently --names \"node,ng\" \"npm run serve:prod\" \"npm run ng-build:prod\"",
    "serve:dev": "nodemon --watch server/**/*.ts --exec ts-node ./server/bin/startup.ts",
    "serve:prod": "set NODE_ENV=production && ts-node ./server/bin/startup.ts",
    "ng-watch": "ng build --dev --watch",
    "ng-build:prod": "ng build --prod",
    "migrations:generate": "ts-node ./node_modules/typeorm/cli.js migrations:generate",
    "migrations:run": "ts-node ./node_modules/typeorm/cli.js migrations:run",
    "migrations:revert": "ts-node ./node_modules/typeorm/cli.js migrations:revert",
    "migrations:init": "npm run schema:drop && npm run migrations:generate -- -n Initial",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:reset": "npm run schema:drop && npm run migrations:run",
    "db:gen-test-data": "npm run schema:reset && ts-node ./server/bin/gen-test-data.ts",
    "test": "set NODE_ENV=testing && mocha -r ts-node/register ./server/**/*.spec.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@sendgrid/mail": "^6.2.1",
    "aws": "0.0.3-2",
    "aws-sdk": "^2.205.0",
    "body-parser": "~1.18.2",
    "chai-http": "^3.0.0",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "express-jwt": "^5.3.0",
    "fs": "0.0.1-security",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.1.1",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "serve-favicon": "~2.4.5",
    "typeorm": "^0.1.13",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.1",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "chai": "^4.1.2",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "mocha": "^5.0.4",
    "nodemon": "^1.15.1",
    "protractor": "~5.1.2",
    "testdouble": "^3.5.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
