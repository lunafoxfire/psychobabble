{
  "name": "soft-skill-testing",
  "version": "0.0.0",
  "scripts": {
    "start": "concurrently --names \"node,ng\" \"npm run serve:dev\" \"npm run ng-watch\"",
    "serve:dev": "nodemon --watch server/**/*.ts --exec ts-node ./server/bin/www.ts",
    "ng-watch": "ng build --dev --watch",
    "migrations:generate": "ts-node ./node_modules/typeorm/cli.js migrations:generate",
    "migrations:run": "ts-node ./node_modules/typeorm/cli.js migrations:run",
    "migrations:revert": "ts-node ./node_modules/typeorm/cli.js migrations:revert",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.1.1",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "serve-favicon": "~2.4.5",
    "typeorm": "^0.1.13",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.1",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.15.1",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
